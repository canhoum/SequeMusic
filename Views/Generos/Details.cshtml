@model SequeMusic.Models.Genero

@{
    ViewData["Title"] = "Detalhes do Gênero";
}

<h1>Detalhes do Gênero</h1>

<div>
    <h4>@Model.Nome</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Descrição</dt>
        <dd class="col-sm-10">@Model.Descricao</dd>
    </dl>
</div>

<a class="btn btn-secondary" asp-action="Index">Voltar</a>
<a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>